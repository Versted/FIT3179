{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 300,
  "layer": [
    {
      "data": {
        "url": "https://versted.github.io/FIT3179/VIC.topojson",
        "format": {
          "type": "topojson",
          "feature": "VIC_LOCALITY_POLYGON_SHP"
        }
      },
      "projection": {
        "type": "equirectangular"
      },
      "mark": {
        "type": "geoshape",
        "fill": "lightgray",
        "stroke": "white"
      }
    },
    {
      "data": {
        "url": "https://versted.github.io/FIT3179/RoadDeath.csv"
      },
      "projection": {
        "type": "equirectangular"
      },
      "mark": "circle",
      "encoding": {
        "longitude": {
          "field": "LONGITUDE",
          "type": "quantitative"
        },
        "latitude": {
          "field": "LATITUDE",
          "type": "quantitative"
        },
        "size": {"value": 10},
        "color": {"value": "steelblue"}
      }
    }
  ]
}
